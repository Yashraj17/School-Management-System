
<div class="container-fluid ps-0">
    <div class="row w-100 p-0">
        <div class="col-2" id="side" style="height:91vh;">
            <%- include('../side') %>
        </div>
        <div class="col-10">
            
            <h4 class="mt-3 border-start border-4 px-2"><%-class_data.class_name %> Students Details </h4>

            <table class="table mt-4">
                <thead class="table-dark">
                  <tr>
                      <th>Roll No. </th>
                      <th>Student Name</th>
                      <th> Father Name</th>
                      <th>Contact</th>
                      <th>Address</th>
                      <th>Payments</th>
                      <th>Action</th>
                  </tr>
                </thead>
                <tbody>
                  
                    <% class_data.students.forEach(data => { %>
                      <tr>
                           <th><%-data.student_id.rollno %></th>
                           <td><%-data.student_id.name %></td>
                           <td><%-data.student_id.father_name %></td>
                           <td><%-data.student_id.contact %></td>
                           <td><%-data.student_id.address %></td>
                           <td>
                            <a href="/admin/delete-patient/ " class="btn btn-sm btn-success">â‚¹ Fee</a>
                           </td>
                           <td>
                            <a href="/admin/delete-patient/ " class="btn btn-sm btn-primary"><i class="bi bi-pencil"></i></i></a>
                            <a href="/admin/delete-patient/ " class="btn btn-sm btn-danger"><i class="bi bi-trash"></i></i></a>
                          </td>
                      </tr>
                      <% }) %>
                </tbody>
              </table>
        </div>
    </div>
  </div>